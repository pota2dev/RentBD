    <script>
        function open_share(title, text, url) {
            let shareData = {
                title: title,
                text: text,
                url: url,
            };
            if (navigator.share && navigator.canShare(shareData)) {
                navigator.share(shareData)
                    .then(() =>
                        console.log('Shared successfully')
                    )
                    .catch((e) =>
                        console.log('Error: ' + e)
                    )
            } else {
                navigator.clipboard.writeText(url);
                alert('Link copied successfull')
            }
        }
    </script>

