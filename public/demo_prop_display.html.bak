    <div class="modal modal-blur fade" id="modal-report-property" tabindex="-1" role="dialog" aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title">Report Property</h5>
                <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
            </div>
            <div class="modal-body">
                <div class="mb-3">
                    <label class="form-label">Select Reason</label>
                    <input type="hidden" name="property_id" id="report-property-id" value="">

                    <!-- <p class="text-muted small">[property id : <span id="report-property-id" class="fw-bold">***</span>]</p> -->
                    <select class="form-select" name="report_reason" id="report-reason-select">
                                                    <option value="1">Property not available</option>
                                                    <option value="2">Phone number unreachable</option>
                                                    <option value="3">Location mismatch</option>
                                                    <option value="4">Rent amount mismatch</option>
                                            </select>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-link link-secondary me-auto" data-bs-dismiss="modal">Cancel</button>
                <button type="button" class="btn btn-primary" id="submit-report-btn">
                    <span class="submit-text">Submit Report</span>
                    <span class="spinner-border spinner-border-sm d-none" role="status" aria-hidden="true"></span>
                </button>
            </div>
        </div>
    </div>
</div>

<script>
    document.addEventListener('DOMContentLoaded', function() {
        const submitBtn = document.getElementById('submit-report-btn');
        const modal = document.getElementById('modal-report-property');
        const propertyIdInput = document.getElementById('report-property-id');
        const reasonSelect = document.getElementById('report-reason-select');

        // Handle modal open event to set property ID
        if (modal) {
            modal.addEventListener('show.bs.modal', function(event) {
                const button = event.relatedTarget;
                const propertyId = button.getAttribute('data-post-id');
                if (propertyId) {
                    propertyIdInput.value = propertyId;
                }
            });
        }

        if (submitBtn) {
            submitBtn.addEventListener('click', function() {
                const propertyId = propertyIdInput.value;
                const reason = reasonSelect.value;

                if (!propertyId) {
                    return;
                }

                // Disable button and show loading
                submitBtn.disabled = true;
                submitBtn.querySelector('.submit-text').textContent = 'Submitting...';
                submitBtn.querySelector('.spinner-border').classList.remove('d-none');

                // Get CSRF token
                const csrfToken = document.querySelector('meta[name="csrf-token"]')?.getAttribute(
                    'content');

                // Get current locale from URL or default to 'bd'
                const locale = window.location.pathname.split('/')[1] || 'bd';

                // Send AJAX request
                fetch(`/${locale}/ajax-report-property/${propertyId}`, {
                        method: 'POST',
                        headers: {
                            'Content-Type': 'application/json',
                            'X-CSRF-TOKEN': csrfToken,
                            'Accept': 'application/json'
                        },
                        body: JSON.stringify({
                            reason: reason
                        })
                    })
                    .then(response => {
                        console.log('Response status:', response.status);
                        if (!response.ok) {
                            throw new Error(`HTTP error! status: ${response.status}`);
                        }
                        return response.json();
                    })
                    .then(data => {
                        console.log('Response data:', data);

                        // Reset button state
                        submitBtn.disabled = false;
                        submitBtn.querySelector('.submit-text').textContent = 'Submit Report';
                        submitBtn.querySelector('.spinner-border').classList.add('d-none');

                        // Clear existing toasts
                        $("#toast_box").empty();

                        // Create toast notification
                        var toastHTML = `
                    <div class="toast custom_toast" role="alert" aria-live="assertive" aria-atomic="true" data-bs-autohide="true">
                        <div class="toast-header text-${data.alert_class}">
                            <span class="avatar avatar-xs me-2" style="background-image: url(https://www.thetolet.com/logo/thetolet-25px.webp)"></span>
                            <strong class="me-auto">${data.title}</strong>
                            <small>Just now</small>
                            <button type="button" class="ms-2 btn-close" data-bs-dismiss="toast" aria-label="Close"></button>
                        </div>
                        <div class="toast-body">${data.message}</div>
                    </div>
                `;

                        $("#toast_box").append(toastHTML);
                        $(".custom_toast").toast('show');

                        // Close modal if successful
                        if (data.alert_type) {
                            try {
                                const bsModal = bootstrap.Modal.getInstance(modal);
                                if (bsModal) {
                                    bsModal.hide();
                                }
                            } catch (e) {
                                console.log('Modal close error (non-critical):', e);
                                // Fallback: just hide the modal manually
                                modal.classList.remove('show');
                                modal.style.display = 'none';

                                document.body.classList.remove('modal-open');
                                document.body.style.removeProperty('overflow');
                                document.body.style.removeProperty('padding-right');

                                document.querySelectorAll('.modal-backdrop').forEach(el => el
                                    .remove());
                            }
                        }
                    })
                    .catch(error => {
                        console.error('Error:', error);

                        // Reset button state
                        submitBtn.disabled = false;
                        submitBtn.querySelector('.submit-text').textContent = 'Submit Report';
                        submitBtn.querySelector('.spinner-border').classList.add('d-none');

                        // Show error toast
                        $("#toast_box").empty();
                        var toastHTML = `
                    <div class="toast custom_toast" role="alert" aria-live="assertive" aria-atomic="true" data-bs-autohide="true">
                        <div class="toast-header text-danger">
                            <span class="avatar avatar-xs me-2" style="background-image: url(https://www.thetolet.com/logo/thetolet-25px.webp)"></span>
                            <strong class="me-auto">Error</strong>
                            <small>Just now</small>
                            <button type="button" class="ms-2 btn-close" data-bs-dismiss="toast" aria-label="Close"></button>
                        </div>
                        <div class="toast-body">An error occurred while submitting your report. Please try again.</div>
                    </div>
                `;
                        $("#toast_box").append(toastHTML);
                        $(".custom_toast").toast('show');
                    });
            });
        }
    });
</script>
